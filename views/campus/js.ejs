    <script>
    $(document).ready(function(){
        $('.delete_build').click(function(){
            if(confirm("確定要刪除?")){
                var id = $(this).val();
                $.ajax({
                    url:    '/campus/delete/'+ id,
                    type:   'GET',
                    error: function(e){
                        console.log("ERR");
                    },
                    success: function(data){
                        $('#b'+ id).remove();
                    }
                })
            }
        })
    })
</script>

