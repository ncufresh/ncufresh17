<script>

    function mouseOver(x) {
        x.style.height = "110px";
        x.style.width = "220px";
    }

    function mouseOut(x) {
        x.style.height = "100px";
        x.style.width = "200px";
    }

    function checkLogin(aTag) {

        if (<%= !!user %>) {
            aTag.href="/personal";
        } else {
            $('#checkLoginModal').modal('show');
        }

    }

    function checkConfirm() {
        window.location.href = '/login';
    }

</script>