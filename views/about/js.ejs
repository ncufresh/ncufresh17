<script>
            
    $('.nav-toggle-btn').on('click', function(e) {
        $('#sideNav').toggleClass('active-nav');
        e.preventDefault();
    });

    function mouseOver(x) {
        x.style.width = "95%";
    }

    function mouseOut(x) {
        x.style.width = "90%";
    }

    function checkLogin(aTag) {

        if (<%= !!user %>) {
            aTag.href="/personal";
        } else {
            $('#checkLoginModal').modal('show');
        }

    }

    function checkConfirm() {
        window.location.href = '/login';
    }

</script>